# --- Copilot prompt (READ-ONLY) ---
# You are setting .env values for a Laravel app that generates a photo book PDF from
# a Nextcloud folder via WebDAV. Provide secure example values. Do not remove existing keys.

APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Nextcloud WebDAV connection
NEXTCLOUD_BASE_URI="https://cloud.example.com/remote.php/dav/files/username/"
NEXTCLOUD_USERNAME="username"
NEXTCLOUD_PASSWORD="app-password-or-token"

# Photobook defaults
PHOTOBOOK_FOLDER="Photos/2025/Family"
PHOTOBOOK_PAPER="a4"
PHOTOBOOK_DPI=150

# Rendering & optimization knobs
PHOTOBOOK_ORIENTATION=landscape
PHOTOBOOK_TARGET_DPI=160
PHOTOBOOK_SAFETY_SCALE=1.15
PHOTOBOOK_OPTIMIZE_RESIZE=true
PHOTOBOOK_OPTIMIZE_MAX_EDGE=
PHOTOBOOK_OPTIMIZE_TO_JPEG=true
PHOTOBOOK_JPEG_QUALITY=72
PHOTOBOOK_PROGRESSIVE_JPEG=true
PHOTOBOOK_FLATTEN_PNG=true
PHOTOBOOK_STRIP_METADATA=true
PHOTOBOOK_EXIF_ORIENTATION=true

# Planner weights
PHOTOBOOK_PLANNER_W_CROP=1.0
PHOTOBOOK_PLANNER_W_ORIENT=0.4
PHOTOBOOK_PLANNER_W_FLOW=0.25
PHOTOBOOK_PLANNER_B_HERO=0.3
PHOTOBOOK_PLANNER_P_HEROMISS=0.05
PHOTOBOOK_PLANNER_P_DIVERSITY=0.15
PHOTOBOOK_PLANNER_P_REPEAT=0.25
PHOTOBOOK_PLANNER_REPEAT_WINDOW=6
PHOTOBOOK_PLANNER_P_HIST=0.12
PHOTOBOOK_PLANNER_TOPK=2
PHOTOBOOK_PLANNER_PICK_RAND=0.25
PHOTOBOOK_PLANNER_SECOND_WITHIN=0.12

# Grouper hero page behavior
PHOTOBOOK_GROUPER_HERO_FREQ=7
PHOTOBOOK_GROUPER_HERO_AR_H=2.2
PHOTOBOOK_GROUPER_HERO_AR_L=0.6
PHOTOBOOK_GROUPER_HERO_QUAL=0.9
PHOTOBOOK_GROUPER_HERO_MP=20.0